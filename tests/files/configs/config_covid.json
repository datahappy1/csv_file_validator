{
  "file_metadata": {
    "file_value_separator": ",",
    "file_row_terminator": "\n",
    "file_has_header": true,
    "file_value_quote_char": "\""
  },
  "file_validation_rules": {
    "file_name_file_mask": ".+\\d+",
    "file_extension": "csv",
    "file_header_column_names": ["SNo","ObservationDate","Province/State","Country/Region",
      "Last Update","Confirmed","Deaths","Recovered"
    ],
    "file_size_range": [
      0,
      1
    ],
    "file_row_count_range": [
      0,
      10000
    ]
  },
  "column_validation_rules": {
    "SNo": {
      "allow_data_type": "int",
      "allow_numeric_value_range": [
        0,
        10000
      ]
    },
    "Country/Region": {
      "allow_data_type": "str",
      "allow_regex":"[a-zA-Z].+"
    },
    "Confirmed": {
      "allow_numeric_value_range": [
        0.0,
        100000.0
      ],
      "allow_data_type": "float"
    },
    "ObservationDate": {
      "allow_data_type": "datetime.%m/%d/%Y"
    },
    "Last Update": {
      "allow_data_type": "datetime.%Y-%m-%dT%H:%M:%S"
    }
  }
}
